# Open Notebook Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPEN_NOTEBOOK_PORT=8502
OPEN_NOTEBOOK_API_PORT=5056
# Important: This URL is used by the browser to connect to the API
# Use the external port (5056), not the internal container port (5055)
OPENNOTEBOOK_API_URL=http://localhost:5056
SURREAL_URL=ws://localhost:8000/rpc
SURREAL_USER=root
SURREAL_PASSWORD=root
SURREAL_NAMESPACE=open_notebook
SURREAL_DATABASE=production

# Open WebUI Configuration
OPEN_WEBUI_PORT=3004
WEBUI_DOCKER_TAG=main-slim

# Ollama Configuration
OLLAMA_PORT=11434
OLLAMA_DOCKER_TAG=latest

# Firecrawl Configuration
FIRECRAWL_PORT=3003
FIRECRAWL_BULL_AUTH_KEY=CHANGEME
# Optional: OpenAI API Key for AI features (JSON format on scrape, /extract API)
FIRECRAWL_OPENAI_API_KEY=
# Optional: Proxy settings
FIRECRAWL_PROXY_SERVER=
FIRECRAWL_PROXY_USERNAME=
FIRECRAWL_PROXY_PASSWORD=
# Optional: SearXNG endpoint for search API
FIRECRAWL_SEARXNG_ENDPOINT=
# Optional: System resource limits (0.0-1.0)
FIRECRAWL_MAX_CPU=0.8
FIRECRAWL_MAX_RAM=0.8